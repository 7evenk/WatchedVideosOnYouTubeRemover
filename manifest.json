{
    "manifest_version": 3,
    "name": "WatchedVideosOnYouTubeRemover",
    "description": "Remove completely watched videos from watch later playlist on YouTube",
    "version": "1.0",
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/playlist?list=WL"
            ],
            "run_at": "document_end",
            "css": [],
            "js": [
                "content.js"
            ]
        }
    ]
}